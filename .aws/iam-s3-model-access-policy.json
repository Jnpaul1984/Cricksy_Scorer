{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ModelBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::cricksy-ml-models",
        "arn:aws:s3:::cricksy-ml-models/*"
      ]
    },
    {
      "Sid": "CloudWatchLogsAccess",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:us-east-1:365183982031:log-group:/cricksy-ai/ml-training:*"
    }
  ]
}
