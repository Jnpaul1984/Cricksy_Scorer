# Production Environment Configuration for Cricksy Scorer

# API Configuration
VITE_API_BASE=https://api.cricksy-scorer.com
VITE_SOCKET_URL=https://api.cricksy-scorer.com

# Application Configuration
VITE_APP_NAME=Cricksy Scorer
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional Cricket Scoring Application

# Production Settings
VITE_DEV_TOOLS=false
VITE_LOG_LEVEL=error

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# UI Configuration
VITE_DEFAULT_OVERS_LIMIT=25
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_SOUND_EFFECTS=true

# Performance Settings
VITE_API_TIMEOUT=30000
VITE_API_RETRIES=3
VITE_CACHE_TTL=600000
